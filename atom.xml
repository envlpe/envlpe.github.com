---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>envl.pe</title>
  <link rel="alternate" type="text/html" href="http://envl.pe/" />
  <link rel="self" type="application/atom+xml" href="http://envl.pe/atom.xml" />
  <id>http://envl.pe/atom.xml</id>
  <updated>{{ site.time | date_to_utc | date:"%Y-%m-%dT%H:%M:%SZ" }}</updated>
  <subtitle>a place for experimentation and then writing about it all</subtitle>

  {% for post in site.posts limit:15 %}
  
  <entry>
    <title>{{ post.title }}</title>
    <link rel='alternate' type='text/html' href='{{ post.url }}' />
    <id>tag:envl.pe:{% if forloop.index0 > last_dash_index %}{{ post.id | replace:"-","_" }}{% else %}{{ post.id }}{% endif %}</id>
    <updated>{{ post.date | date.getutc:"%Y-%m-%dT%H:%M:00Z" }}</updated>
    <updated></updated>

    <author>
      <name>envl.pe</name>
      <uri>http://envl.pe</uri>
      <email>m@envl.pe</email>
    </author>

    <summary>{{ post.content | html_truncatewords:50 }}</summary>
    <content type='html' xml:lang='en' xml:base='http://metajack.im/'>
      <![CDATA[{{ post.content }}{{ post.extended }}]]>
    </content>
  </entry>
  {% endfor %}
</feed>